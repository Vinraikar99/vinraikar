#include<iostream> 
using namespace std; 

class Test1 
{ 
	int x; 
public: 
	void show() { } 
	void print() { }
	
}; 

class Test2 
{ 
	int x; 
public: 
	virtual void show() { } 
	//virtual void print() { }
	
}; 

int main(void) 
{ 
	cout<<sizeof(Test1)<<endl; 
	cout<<sizeof(Test2)<<endl; 
	return 0; 
} 
