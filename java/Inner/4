public class StaticInnerClass2 
{
  static int data=30;  
  static class Inner
  {  
    static void msg()
                    {
                         System.out.println("data is "+data);
                    }  
  }  
  public static void main(String args[])
  {  
  

//no need to create the instance of static nested class  
  }  
  

}


